{
  "name": "sustenta-bag-monolito",
  "version": "1.0.0",
  "description": "Monolithic application for Sustenta Bag project",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "start:watch": "node --watch src/server.js",
    "swagger": "node src/presentation/http/swagger.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "test:all": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:coverage:html": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --coverageReporters=html",
    "test:coverage:unit:html": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit --coverage --coverageReporters=html",
    "coverage:open": "node open-coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sustenta-Bag/sustentabag-backend-monolito.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sustenta-Bag/sustentabag-backend-monolito/issues"
  },
  "homepage": "https://github.com/Sustenta-Bag/sustentabag-backend-monolito#readme",
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.16.1",
    "amqplib": "~0.10.7",
    "bcrypt": "^5.1.1",
    "colors": "~1.4.0",
    "compression": "1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.3.0",
    "helmet": "8.1.0",
    "http-status": "2.1.0",
    "jsonwebtoken": "9.0.2",
    "mapbox": "^1.0.0-beta10",
    "morgan": "1.10.0",
    "multer": "^2.0.1",
    "pg": "^8.14.1",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "~11.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "mockdate": "^3.0.5",
    "nodemon": "^3.1.9",
    "sinon": "^20.0.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.0",
    "swagger-autogen": "2.23.7"
  }
}
